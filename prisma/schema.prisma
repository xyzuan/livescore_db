generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Club {
  id   Int    @id @default(autoincrement())
  name String
  logo String
}

model Article {
  id   Int    @id @default(autoincrement())
  image String
  title String
  body String
}
